{
    "name": "despensa-app/rest-api",
    "type": "project",
    "description": "Despensa-app es una aplicaci√≥n que te permite gestionar productos que compras de forma habitual.",
    "keywords": [
        "laravel"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "require": {
        "php": "8.0.9",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "v2.0.4",
        "guzzlehttp/guzzle": "7.3.0",
        "laravel/framework": "v8.55.0",
        "laravel/tinker": "v2.6.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "v2.10.0",
        "facade/ignition": "2.12.0",
        "fakerphp/faker": "v1.15.0",
        "laravel/homestead": "v12.4.1",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "v5.8.0",
        "phpunit/phpunit": "9.5.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
